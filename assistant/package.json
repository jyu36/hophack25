{
  "name": "research-assistant",
  "version": "1.0.0",
  "description": "AI-powered research assistant for experiment graph management",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "dotenv -e .env -- ts-node src/server/index.ts",
    "dev:server": "dotenv -e .env -- ts-node src/server/index.ts",
    "dev:agent": "dotenv -e .env -- ts-node src/interactive.ts",
    "demo": "dotenv -e .env -- ts-node src/demo.ts",
    "demo:api-test": "dotenv -e .env -- ts-node src/demo-api-test.ts",
    "test:api": "ts-node src/test-api-endpoints.ts",
    "context-demo": "dotenv -e .env -- ts-node src/context-demo.ts",
    "prod": "npm run build && dotenv -e .env -- node dist/server/index.js"
  },
  "dependencies": {
    "@types/nunjucks": "^3.2.6",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "form-data": "^4.0.4",
    "helmet": "^7.1.0",
    "nunjucks": "^3.2.4",
    "openai": "^4.20.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "@types/winston": "^2.4.4",
    "dotenv-cli": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "keywords": [
    "research",
    "ai",
    "assistant",
    "experiment",
    "graph"
  ],
  "author": "HopHack 2025",
  "license": "MIT"
}
